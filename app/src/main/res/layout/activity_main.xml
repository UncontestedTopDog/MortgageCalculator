<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/app_theme_color"    tools:context=".activity.MortgageActivity">    <RelativeLayout        android:id="@+id/title"        android:layout_width="match_parent"        android:layout_height="60dp"        android:background="@color/titile_bg_color">        <TextView            android:layout_width="match_parent"            android:layout_height="match_parent"            android:gravity="center"            android:text="房贷计算器"            android:textColor="@color/title_text_color"            android:textStyle="bold" />        <ImageView            android:id="@+id/history"            android:focusable="true"            android:layout_width="60dp"            android:layout_height="match_parent"            android:layout_alignParentRight="true"            android:padding="20dp"            android:src="@drawable/history" />    </RelativeLayout>    <LinearLayout        android:id="@+id/main_layout"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_below="@+id/title"        android:layout_marginLeft="15dp"        android:layout_marginRight="15dp"        android:layout_marginTop="15dp"        android:orientation="vertical">        <com.example.huangweiliang.mortgagecalculator.view.SelectorView            android:id="@+id/mortgage_type"            android:layout_width="match_parent"            android:layout_height="@dimen/default_item_height"            android:paddingBottom="@dimen/first_bottom_padding"            android:paddingTop="@dimen/first_top_padding"            app:itemSum="3"            app:firstItemText="商业贷款"            app:secondItemText="公积金贷款"            app:thirdItemText="组合贷款"/>        <com.example.huangweiliang.mortgagecalculator.view.SelectorView            android:id="@+id/repayment_type"            android:layout_width="match_parent"            android:layout_height="@dimen/default_item_height"            android:paddingBottom="@dimen/first_bottom_padding"            android:paddingTop="@dimen/first_top_padding"            app:itemSum="2"            app:firstItemText="等额本息"            app:secondItemText="等额本金"/>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="@dimen/default_item_height"            android:paddingBottom="@dimen/first_bottom_padding"            android:paddingTop="@dimen/first_top_padding"            android:orientation="horizontal">            <LinearLayout                android:layout_width="0dp"                android:layout_height="match_parent"                android:layout_weight="1">                <TextView                    android:layout_width="wrap_content"                    android:layout_height="match_parent"                    android:layout_centerVertical="true"                    android:layout_marginRight="10dp"                    android:gravity="center"                    android:text="商&#160;&#160;&#160;&#160;业"                    android:textColor="@color/unselect_text_color"                    android:textSize="@dimen/default_text_size" />                <RelativeLayout                    android:paddingRight="5dp"                    android:layout_weight="1"                    android:layout_width="0dp"                    android:layout_height="match_parent">                    <TextView                        android:text="0&#160;&#160;&#160;&#160;万元"                        android:padding="5dp"                        android:textColor="@color/select_text_color"                        android:textSize="@dimen/default_text_size"                        android:gravity="center_vertical|right"                        android:layout_width="match_parent"                        android:layout_height="match_parent" />                    <com.example.huangweiliang.mortgagecalculator.view.UnitEditText                        android:id="@+id/commercial_et"                        android:layout_width="match_parent"                        android:layout_height="match_parent"                        android:background="@drawable/default_view_bg"                        app:text="100"                        app:unit="&#160;&#160;&#160;&#160;万元" />                </RelativeLayout>            </LinearLayout>            <LinearLayout                android:paddingLeft="5dp"                android:layout_width="0dp"                android:layout_height="match_parent"                android:layout_weight="1">                <TextView                    android:layout_width="wrap_content"                    android:layout_height="match_parent"                    android:layout_centerVertical="true"                    android:layout_marginRight="10dp"                    android:gravity="center"                    android:text="公积金"                    android:textColor="@color/unselect_text_color"                    android:textSize="@dimen/default_text_size" />                <RelativeLayout                    android:layout_width="0dp"                    android:layout_weight="1"                    android:layout_height="match_parent">                    <TextView                        android:text="0&#160;&#160;&#160;&#160;万元"                        android:padding="5dp"                        android:textColor="@color/select_text_color"                        android:textSize="@dimen/default_text_size"                        android:gravity="center_vertical|right"                        android:layout_width="match_parent"                        android:layout_height="match_parent" />                    <com.example.huangweiliang.mortgagecalculator.view.UnitEditText                        android:id="@+id/provident_fund_et"                        android:layout_width="120dp"                        android:layout_height="match_parent"                        android:background="@drawable/default_view_bg"                        android:visibility="gone"                        app:text="100"                        app:unit="&#160;&#160;&#160;&#160;万元" />                </RelativeLayout>            </LinearLayout>        </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="40dp"            android:paddingBottom="10dp"            android:gravity="bottom">            <TextView                android:layout_height="wrap_content"                android:layout_width="0dp"                android:layout_weight="1"                android:text="贷款年限(年)"                android:textColor="@color/unselect_text_color"                android:paddingRight="5dp"                android:textSize="@dimen/default_text_size" />            <TextView                android:id="@+id/mortgage_time"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:paddingRight="20dp"                android:text="20"                android:textColor="@color/select_text_color"                android:textSize="@dimen/default_text_size" />        </LinearLayout>        <SeekBar            android:id="@+id/time_seek"            style="?android:attr/progressBarStyleHorizontal"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:max="29"            android:maxHeight="10dp"            android:minHeight="10dp"            android:progress="19"            android:layout_marginTop="20dp"            android:layout_marginBottom="20dp"            android:progressDrawable="@drawable/common_progressbar"            android:thumb="@drawable/common_thumb" />        <LinearLayout            android:id="@+id/com_rate_picker"            android:layout_width="match_parent"            android:layout_height="40dp">            <TextView                android:layout_width="0dp"                android:layout_height="match_parent"                android:layout_weight="1"                android:text="商业贷款利率(%)"                android:textColor="@color/unselect_text_color"                android:textSize="@dimen/default_text_size" />            <TextView                    android:id="@+id/com_rate_result"                    android:layout_width="wrap_content"                    android:layout_height="match_parent"                    android:paddingRight="20dp"                    android:text="4.9"                    android:textColor="@color/select_text_color"                    android:textSize="@dimen/default_text_size" />        </LinearLayout>        <LinearLayout            android:id="@+id/fund_rate_picker"            android:layout_width="match_parent"            android:layout_height="40dp">            <TextView                android:layout_width="0dp"                android:layout_height="match_parent"                android:layout_weight="1"                android:text="公积金贷款利率(%)"                android:clickable="false"                android:textColor="@color/unselect_text_color"                android:textSize="@dimen/default_text_size" />            <TextView                android:id="@+id/fund_rate_result"                android:layout_width="wrap_content"                android:layout_height="match_parent"                android:paddingRight="20dp"                android:text="3.25"                android:textColor="@color/select_text_color"                android:textSize="@dimen/default_text_size" />        </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="@dimen/default_item_height"            android:gravity="center">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:gravity="center"                android:text="当前年限基准："                android:textColor="@color/unselect_text_color"                android:textSize="@dimen/default_text_size" />            <RelativeLayout                android:layout_width="0dp"                android:layout_height="match_parent"                android:layout_weight="1"                android:gravity="center">                <TextView                    android:id="@+id/commercial_standard_rate"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:gravity="center"                    android:text="商业 4.90%"                    android:textColor="@color/unselect_text_color"                    android:textSize="@dimen/default_text_size" />                <TextView                    android:id="@+id/fund_standard_rate"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentRight="true"                    android:gravity="center"                    android:text="公积金 3.25%"                    android:textColor="@color/unselect_text_color"                    android:textSize="@dimen/default_text_size" />            </RelativeLayout>        </LinearLayout>        <Button            android:id="@+id/calculator"            android:layout_width="match_parent"            android:layout_height="@dimen/default_item_height"            android:background="@drawable/default_btn_bg"            android:text="马上计算"            android:textColor="#FFFFFFFF"            android:textSize="@dimen/default_btn_text_size" />    </LinearLayout>    <com.example.huangweiliang.mortgagecalculator.view.PickDialog        android:id="@+id/pick_dialog"        android:visibility="gone"        android:layout_alignParentBottom="true"        android:layout_width="match_parent"        android:layout_height="wrap_content"/></RelativeLayout>